

<div class="modal fade" role="dialog" id="ContactModal" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog">
        <div class="modal-content" style="max-width:470px;">
            <form method="post" id="Form_ChangeDueDateModal">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Contact Details </h4>
                </div>
                <div class="modal-body">
                    <div class="row">

                        <div class="form-group col-md-12">
                            <label class="control-label form-label col-sm-2"> TransId </label>
                            <div class="col-sm-10">
                                <input class="form-control form-group-margin-bot" type="text" id="Contact-transId" disabled />
                            </div>
                        </div>

                        <div class="form-group col-md-12">
                            <label class="control-label form-label col-sm-2"> Name </label>
                            <div class="col-sm-10">
                                <input class="form-control form-group-margin-bot" name="DtEnd" type="text" id="Contact-Name" disabled />
                            </div>
                        </div>

                        <div class="form-group col-md-12">
                            <label class="control-label form-label col-sm-2"> Mobile </label>
                            <div class="col-sm-10">
                                <button type="button" class="btn btn-default" id="Contact-Mobile"> 09xx xxx xxxx </button>
                            </div>
                        </div>

                        <div class="form-group col-md-12">
                            <label class="control-label form-label col-sm-2"> Reminder Message </label>
                            <div class="col-sm-10">
                                <textarea type="text" class="form-control" id="Contact-ReminderMgs" cols="40" rows="5" > </textarea>
                            </div>
                        </div>

                    </div>
                </div>

                <div class="modal-footer">
                    <div class="col-md-12 col-sm-12 col-xs-9">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        @*<button type="button" class="btn btn-primary" onclick="Submit_DueDateChange()">Submit</button>*@
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>